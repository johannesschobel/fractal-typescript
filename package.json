{
  "description": "A Typescript Framework for fractal",
  "license": "Apache-2.0",
  "repository": "https://github.com/Dakesi95/fractal-typescript",
  "homepage": "https://github.com/Dakesi95/fractal-typescript",
  "bugs": "https://github.com/Dakesi95/fractal-typescript/issues",
  "scripts": {
    "app": "tsc && ts-node src/index.ts",
    "start": "webpack --watch --mode=development",
    "setup": "npm update && npm install",
    "build": "tsc",
    "build-watch": "tsc --watch",
    "test-only": "jest --passWithNoTests",
    "pretest": "npm run build",
    "test": "npm run test-only",
    "clean": "shx rm -rf node_modules/ dist/ docs/",
    "gendoc": "compodoc -p tsconfig.json -n \"Fractal Typescript Documentation\""
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.9",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.2",
    "ts-loader": "^6.0.2",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4"
  },
  "dependencies": {
    "jsonapi-typescript": "^0.1.3"
  }
}
